CFLAGS = -std=gnu99 -w -m32 -D_GNU_SOURCE
LIBS = -lpthread -pthread
SOURCES1 = app1.c
SOURCES2 = app2.c
SOURCES3 = app3.c
SOURCES4 = app4.c
SOURCES5 = app5.c
SOURCES6 = app6.c
